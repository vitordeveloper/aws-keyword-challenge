    <scm>
        <connection>scm:git:https://github.com/vitordeveloper/aws-keyword-challenge.git</connection>
        <developerConnection>scm:git:https://github.com/vitordeveloper/aws-keyword-challenge.git</developerConnection>
        <url>https://github.com/vitordeveloper/aws-keyword-challenge/src</url>
        <tag>HEAD</tag>
    </scm>

    <developers>
        <developer>
            <id>vitor.alvarenga</id>
            <name>Vitor Alvarega</name>
            <email>vitor.rodrigues.alvarenga@gmail.com</email>
            <organization />
            <roles>
                <role>developer</role>
            </roles>
            <timezone>-3</timezone>
        </developer>
